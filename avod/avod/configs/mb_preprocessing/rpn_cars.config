name: 'kitti'
dataset_dir: '/home/amax_yly/Dataset/kitti/object'
data_split: 'trainval'
data_split_dir: 'training'
has_labels: True

cluster_split: 'train',
classes: ['Car']
num_clusters: [1]
bev_source: 'lidar'

kitti_utils_config {
    area_extents: [-40, 40, -5, 3, 0, 70]
    voxel_size: 0.1
    anchor_strides: [0.5, 0.5]

    bev_generator {
        slices {
            height_lo: -0.2
            height_hi: 2.3
            num_slices: 5
        }
    }

    mini_batch_config {
        density_threshold: 1
        use_retinanet: True
        retinanet_config {
            pyramid_levels: ['P2', 'P3', 'P4']
            anchor_scales: [0.5, 1.0, 1.414]
            anchor_ratios: [0.25, 0.5, 1.0, 2.5, 4.0]
            iou_type: '2d'
            iou_2d_thresholds {
                neg_iou_lo: 0.0
                neg_iou_hi: 0.4
                pos_iou_lo: 0.5
                pos_iou_hi: 1.0
            }
        }

    }
}
